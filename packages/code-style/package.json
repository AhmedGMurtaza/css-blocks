{
  "name": "@css-blocks/code-style",
  "version": "0.17.0",
  "main": "configs/tslint.json",
  "readme": "README.md",
  "keywords": [
    "tslint",
    "tslint-plugin"
  ],
  "scripts": {
    "compile": "rm -rf dist && tsc",
    "pretest": "npm run compile",
    "prepublish": "npm run compile && tslint -p . -c tslint.release.json",
    "test": "tslint --test test/rules/*/* && tslint -p .",
    "lint": "tslint -p .",
    "lintfix": "tslint -p . --fix"
  },
  "bugs": {
    "url": "https://github.com/css-blocks/css-blocks/issues"
  },
  "repository": {
    "url": "https://github.com/css-blocks/css-blocks.git",
    "type": "git"
  },
  "homepage": "https://github.com/css-blocks/css-blocks/tree/master/packages/code-style",
  "devDependencies": {
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
