{
  "name": "@css-blocks/jsx",
  "version": "0.1.1",
  "description": "Statically analyzes JSX files for CSS Blocks use.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run build && npm run tslint && mocha dist/test --recursive --opts test/mocha.opts",
    "build": "rm -rf dist && tsc --jsx preserve -p tsconfig.json",
    "watch": "watch 'npm run build' './src' --wait=3",
    "tslint": "tslint --project tsconfig.json --type-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-blocks/jsx-analyzer.git"
  },
  "keywords": [
    "jsx",
    "preact",
    "react",
    "static",
    "analysis",
    "CSS Blocks"
  ],
  "author": "Adam Miller <ammiller@linkedin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/css-blocks/jsx-analyzer/issues"
  },
  "homepage": "https://github.com/css-blocks/jsx-analyzer#readme",
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/babel-traverse": "^6.7.17",
    "@types/babel-types": "^6.7.16",
    "@types/babylon": "^6.16.1",
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "babel-core": "^6.25.0",
    "chai": "^3.5.0",
    "loader-utils": "^1.1.0",
    "mocha": "^3.4.1",
    "mocha-typescript": "^1.1.4",
    "mock-fs": "^4.3.0",
    "mock-require": "^2.0.2",
    "obj-str": "^1.0.0",
    "preact": "^8.2.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.5.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@types/minimatch": "^2.0.29",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.4",
    "minimatch": "^3.0.4",
    "postcss": "^6.0.1",
    "typescript": "^2.3.4"
  }
}
