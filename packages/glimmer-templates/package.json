{
  "name": "@css-blocks/glimmer-templates",
  "version": "0.10.3",
  "description": "Statically analyze & rewrite the css-block styles in Handlebars templates.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "types-local"
  ],
  "scripts": {
    "test": "mocha --opts test/mocha.opts dist/test",
    "compile": "rm -rf dist && tsc -p test/tsconfig.json",
    "pretest": "npm run compile && npm run tslint",
    "prepublish": "npm run compile",
    "tslint": "tslint --project tsconfig.json --type-check",
    "coverage": "istanbul cover -i dist/src/**/*.js --dir ./build/coverage node_modules/mocha/bin/_mocha -- dist/test --opts test/mocha.opts",
    "remap": "remap-istanbul -i build/coverage/coverage.json -o coverage -t html",
    "docs": "typedoc --out ./docs .",
    "watch": "watch 'npm run test' src test types-local --wait=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-block/glimmer-analyzer.git"
  },
  "keywords": [
    "glimmer",
    "static",
    "analysis",
    "handlebars",
    "css-blocks",
    "css",
    "optimizer"
  ],
  "author": "Chris Eppstein <chris@eppsteins.net>",
  "contributors": [
    "Tom Dale <tom@tomdale.net>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/css-blocks/glimmer-analyzer/issues"
  },
  "homepage": "https://github.com/css-blocks/glimmer-analyzer#readme",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/debug": "0.0.29",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-release": "^0.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "remap-istanbul": "^0.9.5",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@glimmer/compiler": "^0.25.1",
    "@glimmer/resolution-map-builder": "0.5.1",
    "@glimmer/resolver": "^0.3.0",
    "@glimmer/syntax": "^0.25.1",
    "@types/glob": "^5.0.30",
    "css-blocks": "^0.10.3",
    "debug": "^2.6.8",
    "glimmer-analyzer": "^0.1.2",
    "glob": "^7.1.2",
    "postcss": "^6.0.1",
    "shelljs": "^0.7.8"
  }
}
